{
  "name": "@sloang/bcrypt-module",
  "version": "1.0.0",
  "description": "A customizable wrapper module to inject a bcrypt service into your nestjs modules",
  "main": "./dist/index.js",
  "private": false,
  "readme": "README.md",
  "scripts": {
    "test": "jest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "nest build"
  },
  "keywords": [
    "nestjs",
    "nest",
    "ts",
    "typescript",
    "password",
    "hash",
    "bcrypt",
    "module"
  ],
  "author": "Sloan Gwaltney",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.7",
    "@nestjs/testing": "^7.0.7",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "bcrypt": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.2.7",
    "prettier": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
